{
	"compilerOptions": {
		"target": "ES2019",
		"lib": ["DOM", "ES2020"],
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "Node",
		"declaration": true,
		"emitDeclarationOnly": false,
		"declarationDir": "dist",
		"outDir": "dist",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true,
		"importHelpers": true
	},
	"include": ["src"],
	"exclude": ["dist", "node_modules"]
}
