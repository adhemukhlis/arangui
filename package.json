{
	"name": "arangui",
	"version": "0.1.1",
	"description": "React UI kit with SCSS, ESM/CJS, and TypeScript types",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.esm.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.esm.js",
			"require": "./dist/index.cjs.js"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"sideEffects": [
		"**/*.scss"
	],
	"scripts": {
		"clean": "rimraf dist",
		"build": "npm run clean && rollup -c",
		"type-check": "tsc --noEmit",
		"prepublishOnly": "npm run build",
		"scss-fix": "yarn sass-migrator module --migrate-deps src/**/*.scss"
	},
	"peerDependencies": {
		"react": ">=17",
		"react-dom": ">=17"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^28.0.6",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@rollup/plugin-typescript": "^12.1.4",
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"postcss": "^8.5.6",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"rimraf": "^5.0.0",
		"rollup": "^4.50.0",
		"rollup-plugin-dts": "^6.2.3",
		"rollup-plugin-postcss": "^4.0.2",
		"sass": "^1.91.0",
		"sass-migrator": "^2.4.2",
		"typescript": "^5.9.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adhemukhlis/my-ui-kit.git"
	},
	"keywords": [
		"react",
		"ui",
		"scss",
		"components",
		"library"
	],
	"author": "adhemukhlis",
	"license": "MIT",
	"dependencies": {
		"tslib": "^2.8.1"
	}
}
